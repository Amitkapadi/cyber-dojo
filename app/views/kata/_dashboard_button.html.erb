
<script type="text/javascript"><!--

$(document).ready(function() {
    
  var dashboardButton = $('#dashboard_button');
  dashboardButton.click(function() {
      cd.postTo('/dashboard/show', {
        id: "<%= @kata.id %>"
      }, '_blank');
  });
  cd.tipify(dashboardButton);
  
});

//--></script>

<button type="button"
	    id="dashboard_button"
        value="<%= t('views.buttons.dashboard') %>">
  <%= t('views.buttons.dashboard') %>
  <div class="tooltip">
    Open a new<br>dashboard page	
  </div>
</input>
