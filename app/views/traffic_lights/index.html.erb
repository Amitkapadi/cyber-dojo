<h1>Listing traffic_lights</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Colour</th>
      <th>Content hash</th>
      <th>Fork count</th>
      <th>Avatarsession</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @traffic_lights.each do |traffic_light| %>
      <tr>
        <td><%= traffic_light.tag %></td>
        <td><%= traffic_light.colour %></td>
        <td><%= traffic_light.content_hash %></td>
        <td><%= traffic_light.fork_count %></td>
        <td><%= traffic_light.avatar_session %></td>
        <td><%= link_to 'Show', traffic_light %></td>
        <td><%= link_to 'Edit', edit_traffic_light_path(traffic_light) %></td>
        <td><%= link_to 'Destroy', traffic_light, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Traffic light', new_traffic_light_path %>
