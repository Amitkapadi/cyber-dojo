
<script language="javascript" type="text/javascript"><!--

$(document).ready(function() {
  
  $('#resume_coding').click(function() {
    cd.run('exists_json', function(id, dojo) {    
      if (!dojo.exists) {
        cd.dialog_cantFindDojo(id);
      } else {    
        cd.dialog_resumeCoding(id);
      }
    });
  });
    
});

//--></script>

<div class="button"
     title="resume practising"
     id="resume_coding">
  resume
</div>