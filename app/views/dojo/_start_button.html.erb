
<script type="text/javascript"><!--

$(document).ready(function() {
  
  $('#start').click(function() {    
    var ok = $(this).data('ok');
    if ($(this).attr('disabled')) {
      return;
    }
    cd.run('start_json', ok, function(id, dojo) {
      if (dojo.full) {        
        cd.dialog(dojo.full_dialog_html, id, ok).dialog('open');
      } else {
        cd.dialog_startCoding(id, ok, dojo.avatar_name, dojo.start_dialog_html).dialog('open');
      }
    });
  });

});

//--></script>

<div class="button"
     title="start practising programming"
     data-ok="<%= t('views.buttons.ok') %>"
     id="start">
  <%= t("views.buttons.start") %>
</div>