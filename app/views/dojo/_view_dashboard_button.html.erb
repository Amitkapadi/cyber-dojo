
<script language="javascript" type="text/javascript"><!--

$j(document).ready(function() {

  $j('#view_dashboard').click(function() {
    $cd.run('exists_json', function(id, dojo) {
      if (!dojo.exists) {
        $cd.dialog_cantFindDojo('view-dashboard', id);
      } else {
        $cd.postTo('/dashboard/show', { id: id });
      }
    });
  });
  
});

//--></script>

<input type="button"
       class="large button"
       id="view_dashboard"
       value="view-dashboard" />
