
<script language="javascript" type="text/javascript"><!--

$(document).ready(function() {

  $('#review_coding').click(function() {
    var id = $.trim($('#kata_id_input').attr('value'));
    if (id === '') {
      cd.dialog_noId();
    } else {    
      cd.run('exists_json', function(id, dojo) {
        if (!dojo.exists) {
          cd.dialog_cantFindDojo(id);
        } else {
          cd.get('/dashboard/show', { id: id }, '_blank');
        }
      });
    }
  });
  
});

//--></script>

<div class="button"
     title="review practising programming"
     id="review_coding">
  review
</div>

