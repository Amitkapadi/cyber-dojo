
<script language="javascript" type="text/javascript"><!--

$(document).ready(function() {

  $('#review_coding').click(function() {
    cd.run('exists_json', function(id, dojo) {
      if (!dojo.exists) {
        cd.dialog_cantFindDojo('review-coding', id);
      } else {
        cd.postTo('/dashboard/show', { id: id }, '_blank');
      }
    });
  });
  
});

//--></script>

<input type="button"
       class="button"
       id="review_coding"
       value="review" />
