
<script language="javascript" type="text/javascript"><!--

$j(document).ready(function() {

  $j('#review_coding').click(function() {
    $cd.run('exists_json', function(id, dojo) {
      if (!dojo.exists) {
        $cd.dialog_cantFindDojo('review-coding', id);
      } else {
        $cd.postTo('/dashboard/show', { id: id });
      }
    });
  });
  
});

//--></script>

<input type="button"
       class="large button"
       id="review_coding"
       value="review-coding" />
