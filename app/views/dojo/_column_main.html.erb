
<table class="align-right">

  <tr class="align-right">
    <td>
      <div id="cyber-dojo">
        <div id="home-page-logo">
          <%= raw home_page_logo(35) %>
        </div>
        cyber&#8209;dojo.org
      </div>
    </td>
  </tr>

  <tr>
    <td>
      <div id="the-place-to-practice-programming">
        <%= raw t("views.labels.the_place_to_practice_programming") %>
      </div>
    </td>
  </tr>

  <tr>
    <td>
      <%= render :partial => 'learn_more_button' %>
    </td>
  </tr>

  <tr>
    <td>
      <div class="button-info">
        choose a language<br/>
        and an exercise<br/>
        to get your id<br/>
      </div>
      <%= render :partial => 'create_button' %>
    </td>
  </tr>

  <tr>
    <td>
      <div class="enter button-info">
        type in your id<br/>
        on <em>each</em> computer<br/>
      </div>
      <%= render :partial => 'enter_id' %>
    </td>
  </tr>

  <tr>
    <td>
      <div class="button-info">
        hit enter<br/>
        on <em>each</em> computer<br/>
      </div>
      <%= render :partial => 'enter_button' %>
    </td>
  </tr>

  <tr>
    <td>
      <div class="button-info">
        click a traffic-light<br/>
        to review its diffs
      </div>
      <%= render :partial => 'review_button' %>
    </td>
  </tr>

  <tr>
    <td>
      <div class="button-info">
        re-enter as a<br/>
        specific animal<br/>
        (occasionally useful)
      </div>
      <%= render :partial => 'reenter_button' %>
    </td>
  </tr>

</table>
