
<table class="align-right">
  <tr class="align-right">
    <td colspan="2">

      <div id="cyber-dojo">
        <div id="home-page-logo">
          <%= raw home_page_logo(35) %>
        </div>
        cyber&#8209;dojo
      </div>

    </td>
  </tr>

  <tr>
    <td colspan="2">
      <div id="a-place-to-practice">
        <%= raw t("views.labels.a_place_to_practice") %>
      </div>
      <div id="programming">
        <%= raw t("views.labels.programming") %>
      </div>
    </td>
  </tr>

  <tr valign="top">
    <td>
      <div id="create-button-info">
        Choose a language<br/>
        and an exercise<br/>
        to get your id<br/>
      </div>
    </td>
    <td>
      <%= render :partial => 'create_button' %>
    </td>
  </tr>

  <tr valign="top">
    <td>
      <div id="enter-id-info">
        Type in your id<br/>
        on <em>each</em> computer<br/>
      </div>
    </td>
    <td>
      <%= render :partial => 'enter_id' %>
    </td>
  </tr>

  <tr valign="top">
    <td>
      <div id="enter-info">
        Hit enter<br/>
        on <em>each</em> computer<br/>
      </div>
    </td>
    <td>
      <%= render :partial => 'enter_button' %>
    </td>
  </tr>

  <tr valign="top">
    <td>
      <div id="review-info">
        Click a traffic-light<br/>
        to review its diffs
      </div>
    </td>
    <td>
      <%= render :partial => 'review_button' %>
    </td>
  </tr>

  <tr valign="top">
    <td>
      <div id="reenter-info">
        Re-enter as a<br/>
        specific animal<br/>
        (occasionally useful)
      </div>
    </td>
    <td>
      <%= render :partial => 'reenter_button' %>
    </td>
  </tr>

</table>
