
<script language="javascript" type="text/javascript"><!--

$(document).ready(function() {
  
  $.each(<%=@buttons.to_json.html_safe%>,
    function(i,name) {
      $('#' + name).click(function() {
        cd['dialog_' + name]();    
    });
  });
  
});

